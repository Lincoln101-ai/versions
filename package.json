{
  "name": "ver",
  "version": "5.1.3",
  "description": "Semantically increment a project's version in multiple files",
  "author": "silverwind <me@silverwind.io>",
  "repository": "silverwind/ver",
  "license": "BSD-2-Clause",
  "bin": {
    "ver": "ver.js"
  },
